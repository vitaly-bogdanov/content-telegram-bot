// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = env("DB_PROVIDER")
  url      = "${env("DB_PROVIDER")}://${env("POSTGRES_USER")}:${env("POSTGRES_PASSWORD")}@${env("DB_HOST")}:${"DB_PORT"}/${env("POSTGRES_DB")}"
}

generator client {
  provider = "prisma-client-js"
}

model Admin {
  id          Int @id @default(autoincrement())
  first_name  String
  last_name   String
  username    String
  password    String?
  telegram_id Int
}