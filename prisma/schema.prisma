// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Admin {
  id          Int @id @default(autoincrement())
  first_name  String
  last_name   String
  username    String
  password    String?
  telegram_id Int @unique
}